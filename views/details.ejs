<!DOCTYPE html>
<html>
    <head>
        <title>HireHub</title>
        
        <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">
        <link href='https://unpkg.com/boxicons@2.1.4/css/boxicons.min.css' rel='stylesheet'>
        <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/css/bootstrap.min.css">
        <link href="https://cdn.jsdelivr.net/npm/remixicon@4.2.0/fonts/remixicon.css" rel="stylesheet"/>
        <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/@fancyapps/ui@5.0/dist/fancybox/fancybox.css"/>
       <script src="https://kit.fontawesome.com/ac4c7d0dfa.js" crossorigin="anonymous"></script>
       
        <link rel="stylesheet" href="/dashboard.css">
    </head>
    <body>
        <div class="sidebar">
            <div class="logo">
                <!-- <img src="logo.png"> -->
            </div>
             <ul class="menu">
                <li class="active">
                        <a href="/dashboard"><i class="fa-solid fa-border-all"></i><span>All Jobs</span></a>
                </li>
                <li>
                    <a href="/technology"><i class="fa-solid fa-laptop-code"></i><span>Technology</span></a>
                </li>
                <li>
                    <a href="/healthcare"><i class="fa-solid fa-user-doctor"></i><span>Healthcare</span></a>
                </li>
                <li>
                    <a href="/education"><i class="fa-solid fa-person-chalkboard"></i> <span>Education</span></a>
                </li>
                <li>
                    <a href="/marketing"><i class="fa-solid fa-magnifying-glass-dollar"></i><span>Marketing</span></a>
                </li>
                  <li >
                    <a href="/construction"><i class="fa-solid fa-helmet-safety"></i> <span>Construction</span></a>
                </li>
                   <li>
                    <a href="/arts"><i class="fa-brands fa-artstation"></i><span>Creative Arts</span></a>
                </li>
               
                    <li>
                    <a href="/others"><i class="fa-solid fa-ellipsis"></i><span>Others</span></a>
                </li>
          
                <!--<li>-->
                <!--    <a href="/settings"><i class="fa fa-cog"></i><span>Settings</span></a>-->
                <!--</li>-->
                <li class="logout">
                    <a href="/logout"><i class="fa fa-sign-out"></i><span>Logout</span></a>
                </li>
            </ul>
        </div>
        <div class="main--content">
            <div class="header--wrapper">
                <div class="header--title">
                    <h3>HireHub</h3>
                    <span>Find the right job for you</span>
                </div>
                
            </div>


  
          <div class="content">
            <!-- <h2 class="text-center">Main Content Area</h2> -->
            <div class=" row ">
          <%contents.forEach ((content, index)=>{ %>
                <div class=" tabular--wrapper col-lg-9 col-sm-12 movie-card  mx-3">
                  <div class="card profile--wrapper service py-3 px-3 border-0">
                      
                    <div class="profile--wrapper">
                        <h2><%= content.compName%></h2>
                    </div>
                  
                 <div class="table-container">
                   <table>
                       <tbody>
                           <tr>
                               <td>Category</td>
                               <td><%= content.category%></td>
                           </tr>
                             <tr>
                               <td>Job Title</td>
                               <td><%= content.jobTitle%></td>
                           </tr>
                           <tr>
                               <td>Age</td>
                               <td><%= content.age%></td>
                           </tr>
                              <tr>
                               <td>Education Level</td>
                               <td><%= content.eduLevel%></td>
                           </tr>
                            <tr>
                               <td>Gender</td>
                               <td><%= content.gender%></td>
                           </tr>
                            <tr>
                               <td>Experience</td>
                               <td><%= content.experience%></td>
                           </tr>
                              <tr>
                               <td>Salary</td>
                               <td><%= content.salary%></td>
                           </tr>
                              <tr>
                               <td>Work Place</td>
                               <td><%= content.workPlace%></td>
                           </tr>
                              <tr>
                               <td>Posted At</td>
                               <td><%= content.postDate%></td>
                             </tr>
                              <tr>
                               <td>Dead Line</td>
                               <td><%= content.deadLine%></td>
                           </tr>
                              <tr>
                               <td>Description</td>
                               <td><%= content.descriptionn%></td>
                           </tr>
                              <tr>
                               <td>Dead Line</td>
                               <td><%= content.deadLine%></td>
                           </tr>
                       </tbody>
                   </table>
                    </div>
               
                   
                      <div class="form-group text-center col-md-12">
                         <button class="btn btn-brand"><a href="/applicationForm">Apply</a></button>
                      </div>
                    
                  </div>
                </div>
            
              
                  <% });
                 %> 
           
           
                <!-- Add more movie cards as needed -->
                  
               
            
              
            </div>
        </div>

     


    </div>
    </body>
</html>